{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "DNS Prefix": {
            "metadata": {
                "description": "DNS Prefix for the cluster"
            },
            "type": "string"
        },
        "Service Principal App ID": {
            "metadata": {
                "description": "App ID of the Service Principal"
            },
            "type": "string"
        },
        "Service Principal Object ID": {
            "metadata": {
                "description": "Object ID of the Service Principal"
            },
            "type": "string"
        },
        "Service Principal Secret": {
            "metadata": {
                "description": "Secret of the Service Principal"
            },
            "type": "securestring"
        }
    },
    "variables": {
        "AKS Cluster Template URL": "https://raw.githubusercontent.com/vplauzon/aks/master/aks-kubenet/deploy.json"
    },
    "resources": [
        {
            "name": "Cluster",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2018-05-01",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "uri": "[variables('AKS Cluster Template URL')]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "DNS Prefix": {
                        "value": "[parameters('DNS Prefix')]"
                    },
                    "Service Principal App ID": {
                        "value": "[parameters('Service Principal App ID')]"
                    },
                    "Service Principal Object ID": {
                        "value": "[parameters('Service Principal Object ID')]"
                    },
                    "Service Principal Secret": {
                        "value": "[parameters('Service Principal Secret')]"
                    }
                }
            }
        }
    ],
    "outputs": {
    }
}